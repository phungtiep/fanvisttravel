{
  "version": 2,

  "routes": [
    { "src": "/api/cars", "dest": "api/services/getCars.js" },
    { "src": "/api/routes", "dest": "api/services/getRoutes.js" },
    { "src": "/api/booking", "dest": "api/route/booking.js" }
  ],



  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|webp|svg|ico|woff2?)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
