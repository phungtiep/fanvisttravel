{
  "version": 2,

  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x",
      "memory": 128
    }
  },

  "routes": [
    { "src": "/api/cars", "dest": "api/server/services/getCars.js" },
    { "src": "/api/routes", "dest": "api/server/services/getRoutes.js" },
    { "src": "/api/booking", "dest": "api/server/route/booking.js" }
  ],



  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|webp|svg|ico|woff2?)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
